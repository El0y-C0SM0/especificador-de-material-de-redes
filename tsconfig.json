{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES6",    
    "jsx": "react-jsx", 
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./src/models",
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node", 
    "esModuleInterop": true, 
    "allowSyntheticDefaultImports": true, 
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/models-source/"]
}